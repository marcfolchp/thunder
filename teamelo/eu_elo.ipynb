   "source": [
    "initial_elo = 1500\n",
    "elo_ratings = {}\n",
    "\n",
    "# Elo Rating Update Function\n",
    "def calculate_elo(home_country, away_country, result, elo_ratings, K=30):\n",
    "    # Ensure both countries are in the Elo dictionary, if not, initialize them\n",
    "    if home_country not in elo_ratings:\n",
    "        elo_ratings[home_country] = initial_elo\n",
    "    if away_country not in elo_ratings:\n",
    "        elo_ratings[away_country] = initial_elo\n",
    "\n",
    "    home_elo = elo_ratings[home_country]\n",
    "    away_elo = elo_ratings[away_country]\n",
    "\n",
    "    # Calculate expected scores\n",
    "    expected_home = 1 / (1 + 10 ** ((away_elo - home_elo) / 400))\n",
    "    expected_away = 1 / (1 + 10 ** ((home_elo - away_elo) / 400))\n",
    "    \n",
    "    # Actual scores (1.0 for home win, 0 for away win, 0.5 for draw)\n",
    "    if result == 1.0:  # Home team wins\n",
    "        actual_home = 1\n",
    "        actual_away = 0\n",
    "    elif result == 0.0:  # Away team wins\n",
    "        actual_home = 0\n",
    "        actual_away = 1\n",
    "    else:  # Draw\n",
    "        actual_home = 0.5\n",
    "        actual_away = 0.5\n",
    "\n",
    "    # Update Elo ratings\n",
    "    new_home_elo = home_elo + K * (actual_home - expected_home)\n",
    "    new_away_elo = away_elo + K * (actual_away - expected_away)\n",
    "    \n",
    "    # Update the Elo ratings dictionary\n",
    "    elo_ratings[home_country] = new_home_elo\n",
    "    elo_ratings[away_country] = new_away_elo\n",
    "    \n",
    "    return elo_ratings\n",
    "\n",
    "# Process each match and update Elo ratings for both home and away countries\n",
    "for index, row in df.iterrows():\n",
    "    home_country = row['homecountry_name']\n",
    "    away_country = row['awaycountry_name']\n",
    "    result = row['result']\n",
    "    elo_ratings = calculate_elo(home_country, away_country, result, elo_ratings)\n",
    "\n",
    "# After all matches, sort countries by their Elo rating (highest first)\n",
    "elo_ranking = sorted(elo_ratings.items(), key=lambda x: x[1], reverse=True)\n",
    "\n",
    "# Create a DataFrame for Elo rankings\n",
    "elo_df = pd.DataFrame(elo_ranking, columns=['Country', 'Elo Rating'])\n",
    "\n",
    "# Display the final Elo ranking\n",
    "print(elo_df)"
   ]
  },
